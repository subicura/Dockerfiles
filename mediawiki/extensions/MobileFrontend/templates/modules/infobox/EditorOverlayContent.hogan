<div class="content">
	{{{spinner}}}
	<div class="editor-interface hidden">
		{{{descriptionLabel}}}
		<input class="mw-ui-input mw-ui-block description" value="{{description}}">
		{{#rows}}
				<strong>{{label}}</strong>
				<div class="values" data-type="{{type}}" data-id="{{id}}">
					{{#values}}
						<div>{{value}}</div>
					{{/values}}
					<div class="editable-field">N/A</div>
				</div>
		{{/rows}}
		<div class="menu-container"></div>
	</div>
</div>