<div class="overlay-header-container position-fixed">
	{{>header}}
</div>

<div class="content talk-section">
	{{{section.text}}}
	{{{spinner}}}
	<div class="comment">
		<div class="list-header">{{reply}}</div>
		<div class="comment-content">
			<textarea class="wikitext-editor"></textarea>
			<p class="license">
				{{info}}
				{{{licenseMsg}}}
			</p>
			<button class="mw-ui-button mw-ui-constructive mw-ui-block save-button">{{saveMsg}}</button>
		</div>
	</div>
</div>
